@{
    Layout = null;
}
@model HandleErrorInfo	
<!DOCTYPE HTML>
<html>
<head>
	<meta charset="UTF-8">
	<meta name="ROBOTS" content="NOINDEX, NOFOLLOW">
	<meta name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1, maximum-scale=2" />
	<title></title>
	<link rel="stylesheet" type="text/css" href="@Url.Content("~/content/css/style.css")" media="all">
	<!--[if lt IE 9]> <script src="js/html5ForIE.js"></script> <![endif]-->
	<!--[if IE 9]> <html class="ie9"> <![endif]-->
	<!--[if IE 8]> <html class="ie8"> <![endif]-->
</head>
<body>
	@Html.Partial("GaScript", "Erreur")
	<header>
		<div></div>
	</header>
	<div class="container">
		<div class="partner partner_1"></div>

        <h3>une erreur s’est produite</h3>
        <p>votre participation n’a pas pu être prise en compte,<br />veuillez revenir plus tard.</p>
        @if (Request.IsLocal || HttpContext.Current.IsDebuggingEnabled)
		{
			if (@Model != null && @Model.Exception != null)
			{
				<p style="font-size:xx-small;" title="@Model.Exception.StackTrace">@Model.Exception.Message</p>					
			}
		}
        <a href="@Url.Action("Index", "Home")" class="btn">Retour à l'accueil</a>
        <div class="clear"></div>
    </div>
</body>
</html>